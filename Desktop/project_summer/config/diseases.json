{
  "diabetes": {
    "name": "diabetes",
    "display_name": "Diabetes Risk",
    "description": "Predict diabetes risk based on patient data",
    "accepted_formats": [
      "csv",
      "json"
    ],
    "parameters": {
      "age": {
        "type": "number",
        "min": 18,
        "max": 120,
        "default": 45,
        "label": "Age"
      },
      "bmi": {
        "type": "number",
        "min": 15.0,
        "max": 50.0,
        "default": 25.0,
        "step": 0.1,
        "label": "BMI"
      },
      "glucose_level": {
        "type": "number",
        "min": 70,
        "max": 300,
        "default": 100,
        "label": "Glucose Level (mg/dL)"
      },
      "blood_pressure": {
        "type": "number",
        "min": 80,
        "max": 200,
        "default": 120,
        "label": "Systolic BP (mmHg)"
      },
      "insulin_level": {
        "type": "number",
        "min": 0,
        "max": 200,
        "default": 80,
        "label": "Insulin Level (\u03bcU/mL)"
      },
      "family_history": {
        "type": "checkbox",
        "default": false,
        "label": "Family History of Diabetes"
      },
      "physical_activity": {
        "type": "selectbox",
        "options": [
          1,
          2,
          3,
          4,
          5
        ],
        "default": 3,
        "label": "Physical Activity Level",
        "help": "1=Very Low, 5=Very High"
      },
      "diet_score": {
        "type": "selectbox",
        "options": [
          1,
          2,
          3,
          4,
          5
        ],
        "default": 3,
        "label": "Diet Quality Score",
        "help": "1=Poor, 5=Excellent"
      },
      "stress_level": {
        "type": "selectbox",
        "options": [
          1,
          2,
          3,
          4,
          5
        ],
        "default": 3,
        "label": "Stress Level",
        "help": "1=Very Low, 5=Very High"
      }
    },
    "model_path": "models/diabetes_model.pkl",
    "is_active": true
  },
